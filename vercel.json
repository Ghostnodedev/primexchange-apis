{
  "version": 2,
  "builds": [
    {
      "src": "src/handler.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/getdata",
      "methods": ["POST"],
      "dest": "/src/handler.js"
    },
    {
      "src": "/register",
      "methods": ["POST"],
      "dest": "/src/handler.js"
    },
    {
      "src": "/getcrypto",
      "dest": "/src/handler.js"
    },
    { "src": "/test", "methods": ["GET"], "dest": "/src/handler.js" }
  ]
}
